{
  "kind": "collectionType",
  "collectionName": "post_tags",
  "info": {
    "name": "Post Tag",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^[\\w-_]*",
      "pluginOptions": {}
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "name"
    },
    "blog_posts": {
      "via": "post_tags",
      "collection": "post"
    },
    "til_posts": {
      "collection": "til",
      "via": "tags",
      "dominant": true
    }
  }
}
